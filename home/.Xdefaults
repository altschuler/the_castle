!! le font
Xft.dpi:                    96
Xft.antialias:              true
Xft.rgba:                   rgb
Xft.hinting:                true
Xft.hintstyle:              hintslight
urxvt*letterSpace:          -1

urxvt*font:                 xft:Source Code Pro:pixelsize=13:antialias=true
urxvt*boldFont:             xft:Source Code Pro:pixelsize=13:antialias=true:bold
urxvt*italicFont:           xft:Source Code Pro:pixelsize=13:antialias=true:italic:autohint=true
urxvt*bolditalicFont:       xft:Source Code Pro:pixelsize=13:antialias=true:bold:italic:autohint=true

urxvt*tintColor:            #ffa500
urxvt*scrollBar:            false
urxvt*saveLines:            1000
*urlLancher:                chromium

urxvt*pointerColorForeground: #93a1a1
urxvt*pointerColorBackground: #586e75
urxvt*cursorColor: #93a1a1

! get community/urxvt-perls
urxvt.perl-ext-common: ...,default,clipboard,url-select,keyboard-select,resize-font
urxvt.keysym.M-c:   perl:clipboard:copy
urxvt.keysym.M-v:   perl:clipboard:paste
urxvt.keysym.M-C-v: perl:clipboard:paste_escaped
URxvt.keysym.M-Escape: perl:keyboard-select:activate
URxvt.resize-font.smaller: C-Down
 URxvt.resize-font.bigger: C-Up

urxvt*saveLines:            10000
urxvt*scrollTtyOutput:      false
urxvt*scrollWithBuffer:     true
urxvt*scrollTtyKeypress:    true

!! wombat theme
